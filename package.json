{
  "name": "prompt-to-graph-workbench",
  "version": "1.0.0",
  "description": "A library for translating natural language prompts to GraphQL queries over Neo4j.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "demo": "ts-node src/demo.ts",
    "demo:advanced": "ts-node src/demo-advanced.ts",
    "demo:catalog": "ts-node src/demo-catalog.ts",
    "test": "jest --coverage"
  },
  "keywords": [
    "graphql",
    "nlp",
    "neo4j",
    "prompt-engineering"
  ],
  "dependencies": {
    "@neo4j/graphql": "^4.0.0",
    "apollo-server-express": "^3.10.0",
    "dotenv": "^16.0.0",
    "graphql": "^16.0.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-validation-complexity": "^0.4.2",
    "neo4j-driver": "^5.0.0",
    "node-fetch": "^2.6.7",
    "openai": "^4.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/graphql-depth-limit": "^1.1.6",
    "@types/graphql-validation-complexity": "^0.4.4",
    "@types/jest": "^29.2.1",
    "@types/node": "^18.11.9",
    "jest": "^29.2.0",
    "ts-jest": "^29.0.3"
  }
}